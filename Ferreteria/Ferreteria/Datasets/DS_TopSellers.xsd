                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT USUARIO.legajo, USUARIO.apellido, USUARIO.nombre, CAST(round(SUM(DETALLE_VENTA.cantidad * DETALLE_VENTA.precioVenta),0) as int) AS 'Vendido'
FROM VENTAS 
	JOIN DETALLE_VENTA ON (VENTAS.numeroVenta = DETALLE_VENTA.numeroVenta)
	JOIN USUARIO ON(VENTAS.vendedor = USUARIO.legajo)
WHERE VENTAS.fecha &gt;= @Desde AND VENTAS.fecha &lt;= @hasta
GROUP BY USUARIO.legajo, USUARIO.apellido, USUARIO.nombre
ORDER BY Vendido DESC
</CommandText>
