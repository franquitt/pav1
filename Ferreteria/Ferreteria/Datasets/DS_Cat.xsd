                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT CLASIFICACION.nombre, SUM(DETALLE_VENTA.cantidad * DETALLE_VENTA.precioVenta) AS 'Vendido'
FROM VENTAS 
	JOIN DETALLE_VENTA ON (VENTAS.numeroVenta = DETALLE_VENTA.numeroVenta)
	JOIN LOTES ON (LOTES.numeroLote = DETALLE_VENTA.numeroLote)
	JOIN PRODUCTOS ON (PRODUCTOS.codigoProducto = LOTES.codigoProducto)
	JOIN CLASIFICACION ON (PRODUCTOS.clasificacion = CLASIFICACION.codigoClasificacion)
WHERE VENTAS.fecha &gt;= @Desde AND VENTAS.fecha &lt;= @hasta
GROUP BY CLASIFICACION.nombre</CommandText>
